{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "noImplicitUseStrict": true
  },
    "exclude": [
        "node_modules/*",
        "typings/index",    
        "typings/index.d.ts",
        "typings/globals/node/index.d.ts"
        ]  
}